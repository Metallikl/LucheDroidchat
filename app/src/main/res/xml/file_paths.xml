<?xml version="1.0" encoding="utf-8"?>
<paths xmlns:android="http://schemas.android.com/apk/res/android">
    <!--Salva imagens em cache possibilitando que o android remova ela se necessario, deve ser usado
        quando for uma imagem que é criada e salva no server por exemplo, sendo ue a original pode morrer
     -->
    <cache-path
        name="cache_images"
        path="."/>

    <!--Salva imagens em definitivo, os sistema não irá apaga-las caso necesside de recurso.
     -->
    <files-path
        name="file_images"
        path="images/"/>

</paths>